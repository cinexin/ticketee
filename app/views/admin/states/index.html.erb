<%= link_to "New State", new_admin_state_path %>

<ul id='states'>
	<% for state in @states %>
		<li id='state_<%= state.id %>'>
			<!-- view "app/config/routes.rb" for details about "make_default_admin_state_path" -->
			<%= link_to  state.name, admin_state_path(state) %> -  <%= link_to "Make default", make_default_admin_state_path(state) %> 
		</li>
	<% end %>
</ul>